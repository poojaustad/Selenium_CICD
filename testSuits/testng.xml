<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="All Test Suite">
    <listeners>
        <listener class-name="rahulshettyacademy.TestComponents.Listeners"></listener>
    </listeners>
    <test verbose="2" preserve-order="true" name="Submit Order Test">
     <classes>
        <class name="rahulshettyacademy.tests.SubmitOrderTest">
          <methods>
             <include name="SubmitOrder"/>
             <include name="OrderHistoryTest"/>
           </methods>
        </class>
     </classes>
    </test>
    <test verbose="2" preserve-order="true" name="Error Validation Test">
        <classes>
            <class name="rahulshettyacademy.tests.ErrorValidationTest">
                <methods>
                    <include name="LoginWrongMailIdValidation"/>
                    <include name="LoginWrongPasswordValidation"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>